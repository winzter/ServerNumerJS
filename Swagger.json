{
  "openapi": "3.0.0",
  "info": {
    "title": "Numerical Method API",
    "description": "Description",
    "version": "1.0.0"
  },
  "host": "localhost:5000",
  "basePath": "/",
  "components": {
    "securitySchemes": {
        "bearerAuth": {
            "type": "http",
            "scheme": "bearer",
            "bearerFormat": "JWT"
        }
    }
  },
  "paths": {
    "/checktoken": {
      "get": {
        "description": "",
        "security": [
          {
            "bearerAuth":[]
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/gentoken/{name}": {
      "get": {
        "description": "",
        "parameters": [
          {
            "name":"name",
            "in":"path",
            "required":"true",
            "schema":{
              "type":"string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/rootofequation/{Topic}": {
      "get": {
        "summary":"Find set of equations",
        "description":"Find set of equations by topic ex. Bisection , OnePoint",
        "parameters": [
          {
            "name":"Topic",
            "in":"path",
            "required":"true",
            "schema":{
              "type":"string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/postdata": {
      "post": {
        "description": "",
        "security": [
          {
            "bearerAuth":[]
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "value": {
                    "type":"string",
                    "example": "Bisection100"
                  },
                  "equation": {
                    "type":"string",
                    "example": "(x+5)-10"
                  },
                  "xl": {
                    "type":"string",
                    "example": "3"
                  },
                  "xr": {
                    "type":"string",
                    "example": "6"
                  },
                  "label": {
                    "type":"string",
                    "example": "(x+5)-10"
                  },
                  "group": {
                    "type":"string",
                    "example": "Example From API"
                  },
                  "topic":{
                    "type":"string",
                    "example":"Bisection"
                  }
                }
              }
            }
          }
        },
        
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  }
  
}